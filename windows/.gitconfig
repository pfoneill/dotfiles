[user]
	name = Paul O'Neill
	email = paul.oneill@abcellera.com
[core]
	excludesFile = ~/.gitignore
	filemode = false
	editor = code --wait
	autocrlf = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[http]
	postBuffer = 157286400
[pull]
	rebase = false
[fetch]
	prune = true
	parallel = 0
[rebase]
	autoStash = false
	autosquash = false
	updateRefs = false
[init]
	defaultBranch = main
[advice]
	detachedHead = false
[alias]
	co = checkout
	br = branch
	st = status
	wt = worktree
[credential "helperselector"]
	selected = manager-core
[safe]
	directory = %(prefix)///csfs02.eng.cayvry1.abcbio.ca/eng/htr/settings/Orion-Settings
	directory = %(prefix)///wsl.localhost/Ubuntu/home/pfoneill/src/hts_team_infrastructure_provisioning
[push]
	autoSetupRemote = true
[submodule]
	fetchJobs = 0
[checkout]
	workers = 0
[credential "https://code.hampel-soft.com"]
	provider = generic
[maintenance]
	repo = C:/src/Orion
[column]
	ui = auto
